{
  "name": "template-aws-repos",
  "version": "0.0.2",
  "description": "Template containing repositories using AWS wrappers built on top of the AWS SDK.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolasdao/template-aws-repos.git"
  },
  "bugs": {
    "url": "https://github.com/nicolasdao/template-aws-repos/issues"
  },
  "homepage": "https://github.com/nicolasdao/template-aws-repos#readme",
  "scripts": {
    "dev": "NODE_ENV=dev node-dev -r sls-config-parser/setenv index.js --inclcreds",
    "prod": "NODE_ENV=dev node-dev -r sls-config-parser/setenv index.js --inclcreds --stage prod --force 'provider.profile=neap_prod;provider.region=ap-southeast-2'",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:prod": "sls deploy --stage prod --aws-profile neap_prod --force",
    "lint": "eslint index.js src/ test/ --fix",
    "push": "git push --follow-tags origin master",
    "rls": "standard-version --release-as",
    "start": "NODE_ENV=production node index.js",
    "test": "mocha --exit",
    "v": "node -e \"console.log(require('./package.json').version)\""
  },
  "author": "Nicolas Dao",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "node-dev": "^4.0.0",
    "serverless-iam-roles-per-function": "^2.0.2",
    "sls-config-parser": "^0.1.2",
    "standard-version": "^6.0.1"
  },
  "dependencies": {
    "aws-sdk": "^2.521.0",
    "core-async": "^1.0.3",
    "form-data": "^2.4.0",
    "node-fetch": "^2.3.0"
  }
}
